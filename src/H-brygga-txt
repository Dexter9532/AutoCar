#include <Arduino.h>

#define IN1 7
#define IN2 8
#define ENA 9

void setup() {
    pinMode(IN1, OUTPUT);
    pinMode(IN2, OUTPUT);
    pinMode(ENA, OUTPUT);
}

void loop() {
    int speed = 150; /*pwm hastighet*/

    /*Kör framåt, förhoppningsvis*/
    digitalWrite(IN1, HIGH);
    digitalWrite(IN2, LOW);
    analogWrite(ENA, speed);

    delay(5000);

    /*stanna*/
    digitalWrite(IN1, LOW);
    digitalWrite(IN2, LOW);
    analogWrite(ENA, 0);

    delay(2000);
}
